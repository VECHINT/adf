{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "df-stg-tbpractice"
		},
		"ls_blb_stg_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_blb_stg'"
		},
		"ls_stg_tb_adf_practice_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_stg_tb_adf_practice'"
		},
		"ls_stg_tb_adf_practice_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "tb-dbserver.database.windows.net"
		},
		"ls_stg_tb_adf_practice_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "stg_tb_adf_practice"
		},
		"ls_stg_tb_adf_practice_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "tbuser"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "df-stg-tbpractice_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_blb_stg_connectionString": {
						"value": "[parameters('ls_blb_stg_connectionString')]"
					},
					"ls_stg_tb_adf_practice_password": {
						"value": "[parameters('ls_stg_tb_adf_practice_password')]"
					},
					"ls_stg_tb_adf_practice_properties_typeProperties_server": {
						"value": "[parameters('ls_stg_tb_adf_practice_properties_typeProperties_server')]"
					},
					"ls_stg_tb_adf_practice_properties_typeProperties_database": {
						"value": "[parameters('ls_stg_tb_adf_practice_properties_typeProperties_database')]"
					},
					"ls_stg_tb_adf_practice_properties_typeProperties_userName": {
						"value": "[parameters('ls_stg_tb_adf_practice_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}